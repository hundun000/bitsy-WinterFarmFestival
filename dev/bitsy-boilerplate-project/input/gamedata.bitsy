    {var_calendarDate = var_calendarDate + 1}
    (replaceAll "TIL, til_outdoor_grass_footprint, TIL, til_outdoor_grass, rm_farm")
    (replaceAll "TIL, til_outdoor_grass_footprint, TIL, til_outdoor_grass, rm_forest")
    (replaceAll "TIL, til_outdoor_grass_footprint, TIL, til_outdoor_grass, rm_lake")
    {
      - var_shippingBinItemsCount > 0 ?
        {var_temp = var_shippingBinItemsCount * const_snowYamSellPrice}
        You sell {print var_shippingBinItemsCount} snow yam(s) and gain {print var_temp}$.
        {item "itm_money" {{item "itm_money"} + var_temp}}
        {var_shippingBinItemsCount = 0}
    }{
      - var_calendarDate > const_goalLimitDate ?
        {
          - {item "itm_money"} >= const_goalMoney ?
            You reach the goal!{pg}
            {var_endSuccessful = true}
            (exitNow "rm_end, 15, 9")
          - else ? 
            You don't reach the goal!{pg}
            {var_endSuccessful = false}
            (exitNow "rm_end, 15, 9")
        }
      - else ?
        Day {print var_calendarDate} comes...
        (randomVarArrayNow "var_snowYamWildSlot_states, 0, 2")
        (imageByVarArrayCondition "SPR, spr_snowYamWildSlot_instances, var_snowYamWildSlot_states, 0, spr_snowYamWildSlot_protytypeEmpty, spr_snowYamWildSlot_protytypeHarvest")
        (randomVarArrayNow "var_wildSpawnState_forest, 8, 2")
        (randomVarArrayNow "var_wildSpawnState_lake, 8, 2")
        (eraseAllNow "ITM, itm_snowYamWildSpawn, rm_forest")
        (eraseAllNow "ITM, itm_snowYamWildSpawn, rm_lake")
        (drawDiscreteByVarArrayCondition "var_wildSpawnState_forest, 8, ITM, 0, itm_snowYamWildSpawn, var_snowYamWildSpawn_spawnX_forest, var_snowYamWildSpawn_spawnY_forest, rm_forest")
        (drawDiscreteByVarArrayCondition "var_wildSpawnState_lake, 8, ITM, 0, itm_snowYamWildSpawn, var_snowYamWildSpawn_spawnX_lake, var_snowYamWildSpawn_spawnY_lake, rm_lake")
        (random2DVarArrayNow "var_winterFieldState, 3, 3, 2")
        (drawBoxBy2DVarArrayCondition "var_winterFieldState, 3,  3, TIL, til_winterField_emptyHide, til_winterField_harvestHide, 9, 4, rm_farm")
    }
}
